<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.esa.beuth.testdriveassist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gui.AppActivity"
            android:label="@string/title_activity_app"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ConnectionOverviewActivity" />
        <activity android:name=".ConnectActivity" />
        <activity android:name=".TestSuiteOverviewActivity" />
        <activity android:name=".TestAssistActivity"></activity>
    </application>

</manifest>